			"fontSizes": [
				{
					"fluid": {
						"max": "0.875rem",
						"min": "0.8rem"
					},
					"name": "Tiny",
					"size": "0.8rem",
					"slug": "tiny"
				},
				{
					"fluid": {
						"max": "1rem",
						"min": "0.875rem"
					},
					"name": "Small",
					"size": "0.875rem",
					"slug": "small"
				},
				{
					"fluid": {
						"max": "1.125rem",
						"min": "1rem"
					},
					"name": "Normal",
					"size": "1rem",
					"slug": "normal"
				},
				{
					"fluid": {
						"max": "1.375rem",
						"min": "1.125rem"
					},
					"name": "Large",
					"size": "1.125rem",
					"slug": "large"
				},
				{
					"fluid": {
						"max": "1.75rem",
						"min": "1.5rem"
					},
					"name": "Extra Large",
					"size": "1.5rem",
					"slug": "extra-large"
				},
				{
					"fluid": {
						"max": "2.5rem",
						"min": "2rem"
					},
					"name": "Huge",
					"size": "2rem",
					"slug": "huge"
				},
				{
					"fluid": {
						"max": "2.75rem",
						"min": "2rem"
					},
					"name": "Gigantic",
					"size": "2rem",
					"slug": "gigantic"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/button": {
				"color": {
					"background": "var:preset|color|foreground",
					"text": "var:preset|color|background"
				}
			},
			"core/code": {
				"border": {
					"color": "#CCCCCC",
					"radius": "0px",
					"style": "solid",
					"width": "2px"
				}
			},
			"core/navigation": {
				"typography": {
					"fontSize": "var:preset|font-size|normal",
					"lineHeight": "1.75"
				},
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "underline"
						}
					}
				}
			},
			"core/post-title": {
				"typography": {
					"fontFamily": "var:preset|font-family|merriweather"
				},
				"css": "font-size-adjust: ex-height 0.553"
			},
			"core/site-tagline": {
				"typography": {
					"fontSize": "var:preset|font-size|normal"
				},
				"spacing": {
					"margin": {
						"top": "0"
					}
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var:preset|font-family|publicsans",
					"fontSize": "var:preset|font-size|huge",
					"fontWeight": "700",
					"textTransform": "uppercase"
				},
				"css": "font-size-adjust: cap-height 0.738",
				"elements": {
					"link": {
						":hover": {
							"color": {
								"text": "currentColor"
							},
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/quote": {
				"border": {
					"color": "var:preset|color|primary"
				}
			},
			"core/list": {
				"css": "padding-inline-start: 1.4rem"
			},
			"core/list-item": {
				"spacing": {
					"padding": {
						"top": "0.375rem"
					}
				}
			}
		},
		"color": {
			"background": "var:preset|color|background",
			"text": "var:preset|color|black"
		},
		"spacing": {
			"blockGap": "1.5rem",
			"padding": {
				"left": "var:preset|spacing|50",
				"right": "var:preset|spacing|50"
			}
		},
		"elements": {
			"button": {
				"color": {
					"background": "var:preset|color|secondary"
				}
			},
			"h1": {
				"typography": {
					"fontFamily": "var:preset|font-family|merriweather",
					"fontSize": "var:preset|font-size|huge"
				}
			},
			"h2": {
				"spacing": {
					"padding": {
						"top": "1.5rem"
					}
				},
				"typography": {
					"fontFamily": "var:preset|font-family|merriweather",
					"fontSize": "var:preset|font-size|extra-large",
					"fontWeight": "400"
				}
			},
			"h3": {
				"spacing": {
					"padding": {
						"top": "1.2rem"
					}
				},
				"typography": {
					"fontFamily": "var:preset|font-family|merriweather",
					"fontSize": "var:preset|font-size|large",
					"fontWeight": "400"
				}
			},
			"h4": {
				"spacing": {
					"padding": {
						"top": "0.6rem"
					}
				},
				"typography": {
					"fontFamily": "var:preset|font-family|merriweather",
					"fontSize": "var:preset|font-size|normal",
					"fontWeight": "700"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var:preset|font-family|merriweather",
					"fontSize": "var:preset|font-size|small",
					"fontWeight": "700"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var:preset|font-family|merriweather",
					"fontSize": "var:preset|font-size|small",
					"textTransform": "uppercase"
				}
			},
			"heading": {
				"color": {
					"text": "var:preset|color|foreground"
				},
				"typography": {
					"fontFamily": "var:preset|font-family|merriweather",
					"lineHeight": "1.2"
				},
				"css": "font-size-adjust: ex-height 0.553"
			},
			"link": {
				":hover": {
					"color": {
						"text": "var:preset|color|foreground"
					}
				},
				"color": {
					"text": "var:preset|color|secondary"
				}
			}
		},
		"typography": {
			"fontFamily": "var:preset|font-family|publicsans",
			"lineHeight": "1.5"
		}
	},
	"templateParts": [
		{
			"name": "sidebar",
			"title": "Sidebar",
			"area": "uncategorized"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	],
	"version": 3
}