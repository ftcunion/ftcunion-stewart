{
	"$schema": "https://schemas.wp.org/wp/6.6/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"duotone": [],
			"gradients": [],
			"palette": [
				{
					"color": "#000",
					"name": "Text",
					"slug": "foreground-text"
				},
				{
					"color": "#061a40",
					"name": "Headers",
					"slug": "foreground-headers"
				},
				{
					"color": "#f1f1f1",
					"name": "background",
					"slug": "background"
				},
				{
					"color": "#a25f00",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#0c6cc1",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#d0262c",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#a35f00",
					"name": "Primary (white on dark)",
					"slug": "primary-text-dark"
				},
				{
					"color": "#0c6cc0",
					"name": "Secondary (white on dark)",
					"slug": "secondary-text-dark"
				},
				{
					"color": "#cf262c",
					"name": "Tertiary (white on dark)",
					"slug": "tertiary-text-dark"
				},
				{
					"color": "#777",
					"name": "Gray",
					"slug": "gray"
				}
			],
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "Tiny",
					"size": "10px",
					"slug": "20"
				},
				{
					"name": "X-Small",
					"size": "20px",
					"slug": "30"
				},
				{
					"name": "Small",
					"size": "30px",
					"slug": "40"
				},
				{
					"name": "Regular",
					"size": "clamp(30px, 5vw, 50px)",
					"slug": "50"
				},
				{
					"name": "Large",
					"size": "clamp(30px, 7vw, 70px)",
					"slug": "60"
				},
				{
					"name": "X-Large",
					"size": "clamp(50px, 7vw, 90px)",
					"slug": "70"
				},
				{
					"name": "XX-Large",
					"size": "clamp(70px, 10vw, 140px)",
					"slug": "80"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"defaultFontSizes": false,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system-ui-serif, New York Small, Charter, Bitstream Charter, Merriweather, Libre Baskerville, Baskerville Old Face, Baskerville, Times New Roman, Times, serif",
					"name": "Transitional Serif",
					"slug": "transitional-serif"
				},
				{
					"fontFamily": "system-ui, sans-serif",
					"name": "System UI",
					"slug": "systemui"
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"max": "0.875rem",
						"min": "0.8rem"
					},
					"name": "Tiny",
					"size": "0.8rem",
					"slug": "tiny"
				},
				{
					"fluid": {
						"max": "1rem",
						"min": "0.875rem"
					},
					"name": "Small",
					"size": "0.875rem",
					"slug": "small"
				},
				{
					"fluid": {
						"max": "1.125rem",
						"min": "1rem"
					},
					"name": "Normal",
					"size": "1rem",
					"slug": "normal"
				},
				{
					"fluid": {
						"max": "1.375rem",
						"min": "1.125rem"
					},
					"name": "Large",
					"size": "1.125rem",
					"slug": "large"
				},
				{
					"fluid": {
						"max": "1.75rem",
						"min": "1.5rem"
					},
					"name": "Extra Large",
					"size": "1.5rem",
					"slug": "extra-large"
				},
				{
					"fluid": {
						"max": "2.5rem",
						"min": "2rem"
					},
					"name": "Huge",
					"size": "2rem",
					"slug": "huge"
				},
				{
					"fluid": {
						"max": "2.75rem",
						"min": "2rem"
					},
					"name": "Gigantic",
					"size": "2rem",
					"slug": "gigantic"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/button": {
				"color": {
					"background": "var:preset|color|foreground-headers",
					"text": "var:preset|color|background"
				}
			},
			"core/code": {
				"border": {
					"color": "#CCCCCC",
					"radius": "0px",
					"style": "solid",
					"width": "2px"
				}
			},
			"core/navigation": {
				"typography": {
					"fontSize": "var:preset|font-size|normal",
					"lineHeight": "1.75"
				},
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "underline"
						}
					}
				}
			},
			"core/post-title": {
				"typography": {
					"fontFamily": "var:preset|font-family|transitional-serif"
				},
				"css": "font-size-adjust: ex-height 0.553"
			},
			"core/site-tagline": {
				"typography": {
					"fontSize": "var:preset|font-size|normal"
				},
				"spacing": {
					"margin": {
						"top": "0"
					}
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var:preset|font-family|systemui",
					"fontSize": "var:preset|font-size|huge",
					"fontWeight": "700",
					"textTransform": "uppercase",
					"lineHeight": "1.2"
				},
				"css": "font-size-adjust: cap-height 0.738",
				"elements": {
					"link": {
						":hover": {
							"color": {
								"text": "currentColor"
							},
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/quote": {
				"border": {
					"color": "var:preset|color|primary"
				}
			},
			"core/list": {
				"css": "padding-inline-start: 1.4rem"
			},
			"core/list-item": {
				"spacing": {
					"padding": {
						"top": "0.375rem"
					}
				}
			}
		},
		"color": {
			"background": "var:preset|color|background",
			"text": "var:preset|color|foreground-text"
		},
		"spacing": {
			"blockGap": "1.5rem",
			"padding": {
				"left": "var:preset|spacing|50",
				"right": "var:preset|spacing|50"
			}
		},
		"elements": {
			"button": {
				"color": {
					"background": "var:preset|color|secondary"
				}
			},
			"h1": {
				"typography": {
					"fontFamily": "var:preset|font-family|transitional-serif",
					"fontSize": "var:preset|font-size|huge"
				},
				"css": "font-size-adjust: ex-height 0.553"
			},
			"h2": {
				"spacing": {
					"padding": {
						"top": "1rem"
					}
				},
				"typography": {
					"fontFamily": "var:preset|font-family|transitional-serif",
					"fontSize": "var:preset|font-size|extra-large",
					"fontWeight": "400"
				},
				"css": "font-size-adjust: ex-height 0.553"
			},
			"h3": {
				"spacing": {
					"padding": {
						"top": "0.75rem"
					}
				},
				"typography": {
					"fontFamily": "var:preset|font-family|systemui",
					"fontSize": "var:preset|font-size|large",
					"fontWeight": "400"
				}
			},
			"h4": {
				"spacing": {
					"padding": {
						"top": "0.667rem"
					}
				},
				"typography": {
					"fontFamily": "var:preset|font-family|systemui",
					"fontSize": "var:preset|font-size|normal",
					"fontWeight": "700"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var:preset|font-family|systemui",
					"fontSize": "var:preset|font-size|small",
					"fontWeight": "700"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var:preset|font-family|systemui",
					"fontSize": "var:preset|font-size|small",
					"textTransform": "uppercase"
				}
			},
			"heading": {
				"color": {
					"text": "var:preset|color|foreground-headers"
				},
				"typography": {
					"lineHeight": "1.2"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var:preset|color|foreground-headers"
					}
				},
				"color": {
					"text": "var:preset|color|secondary"
				}
			}
		},
		"typography": {
			"fontFamily": "var:preset|font-family|systemui",
			"lineHeight": "1.75"
		}
	},
	"templateParts": [
		{
			"name": "sidebar",
			"title": "Sidebar",
			"area": "uncategorized"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	],
	"version": 3
}
