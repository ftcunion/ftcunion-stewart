{
	"$schema": "https://schemas.wp.org/wp/6.6/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"duotone": [],
			"gradients": [],
			"palette": [
				{
					"color": "#1d3557",
					"name": "Foreground",
					"slug": "foreground"
				},
				{
					"color": "#f2f3f4",
					"name": "Background",
					"slug": "background"
				},
				{
					"color": "#414751",
					"name": "Primary",
